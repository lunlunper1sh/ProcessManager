<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Work6-OA
    </title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport" />
    @*<link rel="shortcut icon" href="../../Images/favicon.ico" type="image/x-icon" />*@
    <link rel="stylesheet" href="../../Skins/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="../../Css/font-awesome.min.css" />
    <link rel="stylesheet" href="../../Css/ionicons.min.css" />
    <link rel="stylesheet" href="../../Skins/dist/css/AdminLTE.min.css" />
    <link rel="stylesheet" href="../../Skins/dist/css/skins/skin-blue.min.css" />
    <link rel="stylesheet" href="../../Skins/plugins/iCheck/flat/blue.css" />
    <link rel="stylesheet" href="../../Css/main.css" />
    <!--[if lt IE 9]>
        <script src="../../JS/html5shiv.min.js"></script>
        <script src="../../JS/respond.min.js"></script>
    <![endif]-->
    <script src="../../JS/My97DatePicker/WdatePicker.js"></script>
    <script src="../../JS/mojocube.js"></script>
    <script src="../../Skins/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    @*js数据筛选器*@
    <script src="~/Contents/underscore.js"></script>
    <!--自定义扩展-->
    <script src="~/Contents/ExtentScript.js"></script>
    <!--ligerui-->
    <link href="~/Contents/LigerUI/lib/ligerUI/skins/Aqua/css/ligerui-all.css" rel="stylesheet" />
    <script src="~/Contents/LigerUI/lib/ligerUI/js/ligerui.all.js"></script>
    @RenderSection("PageScript", false)
    @RenderSection("PageStyle", false)
  
</head>

<body class="hold-transition skin-blue  sidebar-collapse">



    <header class="main-header ">


        
            <a id="ctl00_hlLogo" class="logo" href="../"><span class="logo-mini">
                <img src="http://oa.mojocube.com/images/logo_1.png" /></span><span class="logo-lg">Work6-OA</span></a>
        <nav class="navbar navbar-static-top" role="navigation">

            <!-- Navbar Right Menu -->
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">

                    <!-- 通知 -->
                    <li class="dropdown notifications-menu">
                        <a href="../Notice/ViewList.aspx?active=34,37" class="dropdown-toggle" title="通知">
                            <i class="fa fa-bell-o"></i>
                            <span id="ctl00_lblMsgCount" class="label label-warning">1</span>
                        </a>
                    </li>

                    <!-- 邮件 -->
                    <li class="dropdown messages-menu">
                        <a href="../Mail/List.aspx?active=39,42" class="dropdown-toggle" title="邮件">
                            <i class="fa fa-envelope-o"></i>

                        </a>
                    </li>

                    <!-- 任务 -->
                    <li class="dropdown tasks-menu">
                        <a href="../Task/ViewList.aspx?active=46,50" class="dropdown-toggle" title="任务">
                            <i class="fa fa-flag-o"></i>

                        </a>
                    </li>

                    <!-- 用户面板 -->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img id="ctl00_imgPortrait1" class="user-image" src="../../images/userHeader.jpg" style="border-width: 0px;" />
                            <span class="hidden-xs"><span id="ctl00_lblFullName1">盖茨</span></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="user-header">
                                <img id="ctl00_imgPortrait3" class="img-circle" src="../../images/userHeader.jpg" style="border-width: 0px;" />
                                <p>
                                    <span id="ctl00_lblPosition">CEO</span> / 
                      <span id="ctl00_lblDepartment">总公司</span>
                                    <small><span id="ctl00_lblRoleName">超级管理员</span></small>
                                </p>
                            </li>
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="../User/Profile.aspx" class="btn btn-default btn-flat">用户面板</a>
                                </div>
                                <div class="pull-right">
                                    <a href="../Logout.aspx" class="btn btn-default btn-flat" style="background: #D73925; color: #fff; border: solid 1px #D73925">退出登录</a>
                                </div>
                            </li>
                        </ul>
                    </li>

                    <li>
                        <a href="#" data-toggle="control-sidebar"><i class="fa fa-history"></i></a>
                    </li>

                </ul>
            </div>
        </nav>
    </header>




    <!-- 内容 -->
    <div>


        @RenderBody()

    </div>


    <footer class="main-footer">
        <div id="ctl00_Welcome" class="pull-right hidden-xs">2018年02月24日, 星期六</div>
        <strong>Copyright &copy; <span id="ctl00_lblYear">2018</span> <a id="ctl00_hlCopyright" href="#" target="_blank">Work6-OA</a>.</strong> All rights reserved.
      
    </footer>

    @*<div class="control-sidebar-bg"></div>*@

    <script src="../../Skins/plugins/jQueryUI/jquery-ui.min.js"></script>
    <script src="../../Skins/bootstrap/js/bootstrap.min.js"></script>
    <script src="../../Skins/dist/js/app.min.js"></script>

    <script src="../../Skins/plugins/iCheck/icheck.min.js"></script>
    <script>
        $(function () {
            $('input').iCheck({
                checkboxClass: 'icheckbox_flat-blue',
                radioClass: 'iradio_flat-blue',
                increaseArea: '20%'
            });
        });
    </script>

    <script type="text/javascript" src="../../JS/Fancybox/jquery.fancybox.js?v=2.1.5"></script>
    <link rel="stylesheet" type="text/css" href="../../JS/Fancybox/jquery.fancybox.css?v=2.1.5" media="screen" />
    <script type="text/javascript">
        $(document).ready(function () {
            $(".fancybox").fancybox({
                parent: "form:first"
            });
        });
    </script>

    <script src="../../Skins/dist/js/pages/dashboard.js"></script>

</body>
</html>
